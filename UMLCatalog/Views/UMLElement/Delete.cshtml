@model UMLElement

<h3 class="display-4">Вы действительно хотите удалить страницу "@Model.Title"?</h3>
<form asp-action="Delete">
    <input type="hidden" asp-for="Id" />
    <input type="submit" value="Delete" class="btn btn-danger" /> |
    <a asp-action="Index" asp-controller="Home">Вернуться на главную страницу</a>
</form>